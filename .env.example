# .env.example

# Public Supabase Anonymous Key (for client-side usage)
PUBLIC_SUPABASE_ANON_KEY=your-public-supabase-anon-key-here

# Supabase Service Role Key (for server-side usage; do not expose publicly)
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key-here

# Public Supabase URL
PUBLIC_SUPABASE_URL=https://your-supabase-project.supabase.co

# Public Frontend URL (used for CORS, redirection, etc.)
PUBLIC_FRONTEND_URL=http://localhost:5173

# OpenAI API Key
OPENAI_API_KEY=your-openai-api-key-here
